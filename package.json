{
  "name": "write-good-linter",
  "displayName": "Write Good Linter",
  "description": "Applies the Write Good Linter to you Markdown, so you can write more good.",
  "version": "0.0.2",
  "publisher": "travisthetechie",
  "engines": {
    "vscode": "^0.10.6"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.7.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "write-good": "^0.9.1"
  },
  "license": "MIT",
  "repository": {
      "url": "https://github.com/TravisTheTechie/vscode-write-good"
  }
}
